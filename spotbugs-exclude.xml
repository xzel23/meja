<FindBugsFilter>

  <!-- false positive in Java 11, see https://github.com/spotbugs/spotbugs/issues/756 -->
  <Match>
    <Or>
      <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE" />
      <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE" />
    </Or>
  </Match>

</FindBugsFilter>
